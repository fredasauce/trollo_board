<h1><%= @board.board_name %> - <%= @list.list_name %> </h1>
<hr />

<%= link_to "View Tasks", list_tasks_path(@list), class: "ui green button" %>
<%= link_to "Edit List", edit_board_list_path(@board, @list), class: "ui blue button" %>
